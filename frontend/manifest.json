{
  "name": "Chongluadao",
  "version": "1.0.01",
  "description": "chong lua dao",
  "permissions": [
    "tabs",
    "storage",
    "webRequestBlocking",
    "webRequest",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/randomforest.js",
      "js/background.js",
      "js/rating.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "plugin_ui.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["js/jquery.js", "js/features.js"]
  }],
  "web_accessible_resources": [
    "js/blocking.js",
    "blocking.html"
  ],
  "incognito": "split",
  "icons": {
    "16": "assets/cldvn16.png",
    "32": "assets/cldvn32.png",
    "48": "assets/cldvn48.png",
    "128": "assets/cldvn128.png"
  },
  "manifest_version": 2
}